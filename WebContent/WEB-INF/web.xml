<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <servlet>
    <servlet-name>txtRetu</servlet-name>
    <servlet-class>webChat.java.txtRetu</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>login</servlet-name>
    <servlet-class>login.loginForm</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>txtRetu</servlet-name>
    <url-pattern>/webChat</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>login</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
</web-app>